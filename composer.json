    "autoload-dev": {
        "psr-4": {
            "\"Programic\\\\LaravelConvertCaseMiddleware\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    }
}
